<h1>Listing editions</h1>

<table>
  <tr>
    <th>Start</th>
    <th>Finish</th>
    <th>Location</th>
    <th>Active</th>
    <th>Seats</th>
    <th>Price in cents</th>
    <th>Course</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @editions.each do |edition| %>
  <tr>
    <td><%= edition.start %></td>
    <td><%= edition.finish %></td>
    <td><%= edition.location %></td>
    <td><%= edition.active %></td>
    <td><%= edition.seats %></td>
    <td><%= edition.price_in_cents %></td>
    <td><%= edition.course %></td>
    <td><%= link_to 'Show', edition %></td>
    <td><%= link_to 'Edit', edit_edition_path(edition) %></td>
    <td><%= link_to 'Destroy', edition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Edition', new_edition_path %>
